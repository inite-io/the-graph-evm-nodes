{
  "name": "bsc-not-main",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create-local": "graph create --node http://localhost:8020/ werber/bsc-not-main-four",
    "remove-local": "graph remove --node http://localhost:8020/ werber/bsc-not-main-four",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 werber/bsc-not-main-four",

    "bsc:template:testnet": "rm ./subgraph.yaml || true && cp ./subgraph_template.yaml ./subgraph.yaml && sed -i \"s/{{ADDRESS}}/0xF7cAD1f9De025022BB370Bc59D9B2f4cE507771d/\" ./subgraph.yaml && sed -i \"s/{{START_BLOCK}}/17393453/\" ./subgraph.yaml",
    "bsc:deploy:testnet": "yarn bsc:template && yarn codegen && yarn build && graph deploy --node https://api.thegraph.com/deploy/ werber/bsc-test-net",
    "polygon:template:testnet": "rm ./subgraph.yaml || true && cp ./subgraph_template.yaml ./subgraph.yaml && sed -i \"s/{{ADDRESS}}/0xB154e9F0EA0509DBF4c8bf40a93886ddBb0FA435/\" ./subgraph.yaml && sed -i \"s/{{START_BLOCK}}/25362952/\" ./subgraph.yaml",
    "polygon:deploy:testnet": "yarn bsc:template && yarn codegen && yarn build && graph deploy --node https://api.thegraph.com/deploy/ werber/polygon-staging",

    "aurora:template:testnet": "./scripts/aurora/deploy_testnet.sh",
    "aurora:deploy:testnet": "yarn aurora:template:testnet && yarn codegen && yarn build && graph deploy --node https://api.thegraph.com/deploy/ werber/aurora-staging"

  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.28.0",
    "@graphprotocol/graph-ts": "0.26.0"
  }
}
